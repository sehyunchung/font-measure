{
  "name": "font-measure",
  "reason": {
    "react-jsx": 3
  },
  "sources": {
    "dir": "src",
    "subdirs": true,
    "generators": [
      {
        "name": "gen-tailwind",
        "edge": ["tailwind.css", ":", "index.css"]
      }
