    {
      "module": "commonjs",
      "in-source": true
    }
  ],
  "suffix": ".bs.js",
  "namespace": true,
  "bs-dependencies": ["reason-react"],
  "ppx-flags": [["tailwind-ppx", "-path src/tailwind.css"]],
  "refmt": 3,
  "warnings": {
    "error": "+5",
    "number": "-44"
  },
  "bsc-flags": ["-bs-super-errors", "-bs-no-version-header", "-open Belt"]
}
