{
  "name": "font-measure",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.0.0",
    "@tailwindcss/postcss7-compat": "^2.0.2",
    "autoprefixer": "^9",
    "bs-css-emotion": "1.2.0",
    "bs-platform": "8.2.0",
    "concurrently": "5.3.0",
    "font-measure": "^1.2.2",
    "postcss": "^7",
    "postcss-cli": "^8.3.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "reason-react": "0.9.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat"
  },
  "scripts": {
    "start": "concurrently \"yarn bsb\" \"craco start\"",
    "build": "yarn bsb:build && craco build",
    "build:styles": "tailwind build src/index.css -o src/tailwind.css",
    "test": "craco test",
    "eject": "react-scripts eject",
    "start:re": "bsb -clean-world -make-world -w",
    "bsb:build": "bsb -make-world",
    "bsb:clean": "bsb -clean",
    "postinstall": "yarn bsb:build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@dylanirlbeck/tailwind-ppx": "^0.8.4"
  }
}
