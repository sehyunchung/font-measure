    "concurrently": "5.3.0",
    "font-measure": "^1.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "reason-react": "0.9.1"
  },
  "scripts": {
